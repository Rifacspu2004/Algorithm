#include <stdio.h>

// Normal multiplication for 2x2 matrix
void normal2x2(int A[2][2], int B[2][2], int C[2][2]) {
    C[0][0] = A[0][0] * B[0][0] + A[0][1] * B[1][0];
    C[0][1] = A[0][0] * B[0][1] + A[0][1] * B[1][1];
    C[1][0] = A[1][0] * B[0][0] + A[1][1] * B[1][0];
    C[1][1] = A[1][0] * B[0][1] + A[1][1] * B[1][1];
}

// Strassen’s multiplication for 2x2
void strassen2x2(int A[2][2], int B[2][2], int C[2][2]) {
    int p, q, r, s, t, u, v;

    p = (A[0][0] + A[1][1]) * (B[0][0] + B[1][1]);
    q = (A[1][0] + A[1][1]) * B[0][0];
    r = A[0][0] * (B[0][1] - B[1][1]);
    s = A[1][1] * (B[1][0] - B[0][0]);
    t = (A[0][0] + A[0][1]) * B[1][1];
    u = (A[1][0] - A[0][0]) * (B[0][0] + B[0][1]);
    v = (A[0][1] - A[1][1]) * (B[1][0] + B[1][1]);

    C[0][0] = p + s - t + v;
    C[0][1] = r + t;
    C[1][0] = q + s;
    C[1][1] = p + r - q + u;
}

// Normal multiplication for n×n
void normalMultiply(int n, int A[10][10], int B[10][10], int C[10][10]) {
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            C[i][j] = 0;
            for (int k = 0; k < n; k++) {
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }
}

int main() {
    int n;
    printf("Enter matrix size (n x n): ");
    scanf("%d", &n);

    int A[10][10], B[10][10], C[10][10];

    printf("Enter matrix A:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &A[i][j]);

    printf("Enter matrix B:\n");
    for (int i = 0; i < n; i++)
        for (int j = 0; j < n; j++)
            scanf("%d", &B[i][j]);

    if (n == 2) {
        // Use Strassen for 2x2
        int A2[2][2], B2[2][2], C2[2][2];
        for(int i=0;i<2;i++) for(int j=0;j<2;j++){ A2[i][j]=A[i][j]; B2[i][j]=B[i][j]; }
        strassen2x2(A2, B2, C2);
        for(int i=0;i<2;i++) for(int j=0;j<2;j++) C[i][j]=C2[i][j];
    } else {
        // Normal O(n^3) multiply for larger matrices
        normalMultiply(n, A, B, C);
    }

    printf("Result matrix C:\n");
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("%d ", C[i][j]);
        }
        printf("\n");
    }

    return 0;
}
